  shorttitle = {Generalized evaluation of environmental radioactivity measurements with {{UncertRadio}}. {{Part I}}},
  author = {Kanisch, G{\"u}nter},
  year = {2016},
  month = apr,
  journal = {Applied Radiation and Isotopes},
  volume = {110},
  pages = {28--41},
  issn = {0969-8043},
  doi = {10.1016/j.apradiso.2015.12.003},
  url = {https://www.sciencedirect.com/science/article/pii/S0969804315303419},
  urldate = {2023-11-28},
  abstract = {It is shown how a generalized evaluation of a large variety of environmental radioactivity measurements, without and with using linear unfolding, can be performed with a single program, UncertRadio (UR). Using a function parser allows deriving numerical partial derivatives for ISO GUM compatible uncertainty propagation. The evaluation within UR is extended to include ISO 11929 decision thresholds and detection limits. Alternatively, propagation of distributions with MC simulation is included. Part I gives an overview considering evaluations without using linear unfolding.},
  keywords = {Function parser,ISO 11929 detection limit calculation,ISO GUM,Monte Carlo simulation,Numerical uncertainty estimation},
  file = {/home/oberf/Zotero/storage/KIJJCYQC/Kanisch - 2016 - Generalized evaluation of environmental radioactiv.pdf;/home/oberf/Zotero/storage/IKFD85IA/S0969804315303419.html}
}

